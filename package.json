{
  "name": "peaql",
  "version": "0.1.16",
  "description": "PeaQL - A query language for data",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "test:run": "vitest run",
    "fmt": "prettier ./ --write",
    "check": "npm run lint && npm run check-types && npm run test:run",
    "tag": "npm run check && npm run build && npm version patch -m \"chore: bump version to %s\" && git push --follow-tags"
  },
  "exports": {
    "import": "./dist/index.js"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@codemirror/language": "^6.11.3",
    "@lezer/lr": "^1.4.2",
    "luxon": "^3.5.0"
  },
  "dependencies": {
    "big.js": "^7.0.1",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@lezer/generator": "^1.7.2",
    "@types/big.js": "^6.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.7.1",
    "@types/md5": "^2.3.5",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dedent": "^1.6.0",
    "eslint": "^9.20.1",
    "globals": "^16.3.0",
    "md5": "^2.3.0",
    "peggy": "^5.0.6",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.2",
    "publint": "^0.3.12",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.1",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.2.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevejuma/peaql.git"
  }
}
